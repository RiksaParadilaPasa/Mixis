<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mixis">

    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logomixi"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Help"></activity>
        <activity android:name=".Member" />
        <activity android:name=".ArtikelDetail" />
        <activity android:name=".Artikel" />
        <activity android:name=".TukarPointDetail" />
        <activity android:name=".TukarPoint" />
        <activity android:name=".TentangKami" />
        <activity android:name=".VisiMisi" />
        <activity android:name=".AgendaDetail" />
        <activity android:name=".Agenda" />
        <activity android:name=".Mixi" />

        <meta-data
            android:name="com.google.android.gms.ads.AD_MANAGER_APP"
            android:value="true" />

        <activity
            android:name=".Splashscreen"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Notif" />
        <activity android:name=".Trans" />
        <activity android:name=".Profile" />
        <activity android:name=".Cart" />
        <activity android:name=".Upload" />
        <activity android:name=".Login" />
        <activity android:name=".Signup" />
        <activity
            android:name=".Store"
            android:label="@string/title_activity_store_scrolling"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Detail" />
        <activity android:name=".Status" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>